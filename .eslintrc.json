{
	"extends": "wikimedia",
	"plugins": [
        "jsdoc"
    ],
	"env": {
		"browser": true,
		"jquery": true
	},
	"globals": {
		"AFCH": "writable",
		"Hogan": "readonly",
		"importScript": "readonly",
		"mediaWiki": "writable",
		"mw": "writable",
		"OO": "readonly"
	},
	"rules": {
		"camelcase": "off",
		"eqeqeq": "warn",
		"jsdoc/require-param": "warn",
		"jsdoc/require-param-type": "warn",
		"jsdoc/require-returns": "warn",
		"max-len": "off",
		"no-console": "warn",
		"no-global-assign": "warn",
		"no-implicit-globals": "warn",
		"no-new": "warn",
		"no-prototype-builtins": "warn",
		"no-shadow": "warn",
		"no-undef": "warn",
		"no-underscore-dangle": "warn",
		"no-unused-vars": "warn",
		"no-use-before-define": "warn"
	},
	"settings": {
		"jsdoc": {
			"ignoreInternal": true
		}
	}
}
